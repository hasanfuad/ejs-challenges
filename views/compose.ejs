<%- include("partials/header") %>

<form action="/compose" method="post">
    
    <h1>Compose</h1>
    <div class="form-group">
        <label for="title">Title</label>
        <input type="text" class="form-control" rows="5" id="comment" name="inputTitle">
        <label for="comment">Post</label>
        <textarea type="text" class="form-control" rows="5" cols="30" id="comment" name="inputArea"><%= composeInput%> </textarea>
        
    </div>
    <button type="submit" class="btn btn-primary" name="publishButton">Publish</button>

</form>
<%- include("partials/footer") %>